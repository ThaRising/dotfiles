[plugins/media-keys/custom-keybindings/custom0]
binding='<Super>t'
command='jumpapp kitty'
name='Launch Terminal'

[plugins/media-keys/custom-keybindings/custom1]
binding='<Super>b'
command='jumpapp chromium'
name='Launcher Browser'

[plugins/media-keys/custom-keybindings/custom2]
binding='Print'
command='flameshot gui'
name='Screenshot'

[plugins/media-keys/custom-keybindings/custom3]
binding='<Super>n'
command='jumpapp notion-desktop'
name='Launch Notion'

[plugins/media-keys/custom-keybindings/custom4]
binding='<Super>l'
command='dbus-send --type=method_call --dest=org.gnome.ScreenSaver /org/gnome/ScreenSaver org.gnome.ScreenSaver.Lock'
name='Lock Screen'
